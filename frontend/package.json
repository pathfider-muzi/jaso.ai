{
  "name": "jaso.ai",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 8080",
    "prestart": "yarn build",
    "server": "node server.js",
    "preserver": "yarn build",
    "lint": "next lint",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "tsc": "tsc -w -noEmit",
    "postinstall": "cd .. && husky install frontend/.husky",
    "pm2:start": "yarn build && pm2 start pm2.config.js"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "axios": "^0.24.0",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.34.8",
    "react-select": "^5.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@emotion/babel-plugin": "^11.7.2",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@types/node": "17.0.8",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.3",
    "babel-plugin-emotion": "^11.0.0",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.7",
    "eslint-plugin-storybook": "^0.5.5",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "typescript": "4.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cd .. && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}